{
	"cmp":{
		"confirmCert":false,
		"sendCaCert":false,
		"sendCertChain":false,
		"messageTimeRequired":true,
		"sendResponderCert":true,
		"messageTimeBias":300,
		"confirmWaitTime":300,
		"requestSigAlgos":[
			"SHA256withRSA",
			"SHA256withECDSA"
		],
		"requestPbmMacs":[
			"HMAC-SHA256"
		],
		"requestPbmOwfs":[
			"SHA256"
		],
		"responsePbmMac":"HMAC-SHA256",
		"responsePbmOwf":"SHA256",
		"responsePbmIterationCount":10240
	},
	// WARNING: Replace the dummy authenticator with your own.
	"authenticator":"org.xipki.ca.gateway.dummy.DummyRequestorAuthenticator",
	"signers":{
		"default":{
			"type":"pkcs12",
			"conf":"password=$[gateway.https.keyPassword],keystore=file:keycerts/gateway-server.p12,algo=SHA256withECDSA"
		}
	}
}
