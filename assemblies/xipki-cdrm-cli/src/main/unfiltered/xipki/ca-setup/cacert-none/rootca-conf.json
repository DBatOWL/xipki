{
	"basedir":"APP_DIR",
	"properties":{
		"profiles.dir":"xipki/ca-setup/certprofile",
		"token.type":"pkcs12",
		"ca.signerConf":"algo=SM3withSM2,password=1234,keystore=file:xipki/ca-setup/keycerts/rootca1.p12"
	},
	"signers":[{
		"name":"responder1",
		"type":"pkcs12",
		"conf":{
			"value":"password=1234,keystore=file:xipki/keycerts/tlskeys/server/tls-server.p12,algo=SHA256withRSA"
		}
	}],
	"requestors":[{
		"name":"requestor1",
		"type":"cert",
		"binaryConf":{
			"file":"xipki/keycerts/tlskeys/client/tls-client-cert.der"
		}
	},{
		"name":"requestor-mac1",
		"type":"pbm",
		"conf":{
			"value":"123456"
		}
	}],
	"users":[{
		"name":"user1",
		"active":true,
		"password":"password1"
	}],
	"publishers":[],
	"profiles":[{
		"name":"rootca",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-cdrm-rootca.json"
		}
	},{
		"name":"server-subca",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-cdrm-server-subca.json"
		}
	},{
		"name":"client-subca",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-cdrm-client-subca.json"
		}
	},{
		"name":"client",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-cdrm-client.json"
		}
	},{
		"name":"server",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-cdrm-server.json"
		}
	},{
		"name":"contentencryption",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-cdrm-contentencryption.json"
		}
	},{
		"name":"keygateway",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-cdrm-keygateway.json"
		}
	},{
		"name":"keymanagement",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-cdrm-keymanagement.json"
		}
	},{
		"name":"ocsp",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-cdrm-ocsp.json"
		}
	}],
	"cas":[{
		"name":"rootca1",
		"aliases":[
			"rootca"
		],
		"caInfo":{
			"caUris":{
			},
			"cmpControl":{
				"send.ca":true,
				"messagetime.bias":300,
				"rr.aki.required":true,
				"group.enroll":false,
				"protection.sigalgo":"SHA256withRSA:SHA256withECDSA",
				"popo.sigalgo":"SM3withSM2"
			},
			"cmpResponderName":"responder1",
			"crlControl":{
				"update.mode":"interval",
				"fullcrl.intervals":7,
				"overlap.days":3,
				"interval.time":"02:00"
			},
			"duplicateKey":false,
			"duplicateSubject":true,
			"expirationPeriod":365,
			"genSelfIssued":{
				"certOutform":"der",
				"csr":{
					"file":"xipki/ca-setup/keycerts/rootca1.csr"
				},
				"profile":"rootca",
				"serialNumber":"1"
			},
			"keepExpiredCertDays":-1,
			"maxValidity":"99y",
			"nextCrlNo":2,
			"permissions":["all"],
			"protocolSupport":["cmp","rest"],
			"saveReq":false,
			"scepControl":{
				"cacert.included":true,
				"signercert.included":true,
				"support.getcrl":true
			},
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":4,
			"status":"active",
			"validityMode":"cutoff"
		},
		"profiles":[
			"server-subca",
			"client-subca"
		],
		"publishers":[
		],
		"requestors":[{
			"ra":true,
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		},{
			"ra":true,
			"requestorName":"requestor-mac1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}],
		"users":[{
			"userName":"user1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
