{
	"basedir":"APP_DIR",
	"properties":{
		"token.type":"REPLACEME-TOKEN_TYPE",
		"ca.signerConf":"REPLACEME-CA_SIGNERCONF"
	},
	"cas":[{
		"name":"server-ca1",
		"aliases":[
			"server-ca"
		],
		"caInfo":{
			"caUris":{
			},
			"certchain":[{
				"file":"xipki/ca-setup/keycerts/rootca1.der"
			}],
			"cmpControl":{
				"send.ca":true,
				"messagetime.bias":300,
				"rr.aki.required":true,
				"group.enroll":false,
				"protection.sigalgo":"SHA256withRSA:SHA256withECDSA",
				"popo.sigalgo":"SM3withSM2"
			},
			"cmpResponderName":"responder1",
			"crlControl":{
				"update.mode":"interval",
				"fullcrl.intervals":7,
				"overlap.days":3,
				"interval.time":"02:00"
			},
			"duplicateKey":false,
			"duplicateSubject":true,
			"expirationPeriod":365,
			"cert":{
				"file":"xipki/ca-setup/keycerts/server-ca1.der"
			},
			"keepExpiredCertDays":-1,
			"maxValidity":"5y",
			"nextCrlNo":2,
			"permissions":["all"],
			"protocolSupport":["cmp","rest"],
			"saveReq":false,
			"scepControl":{
				"cacert.included":true,
				"signercert.included":true,
				"support.getcrl":true
			},
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":4,
			"status":"active",
			"validityMode":"cutoff"
		},
		"profiles":[
			"contentencryption",
			"keygateway",
			"keymanagement",
			"ocsp",
			"server"
		],
		"publishers":[
		],
		"requestors":[{
			"ra":true,
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		},{
			"ra":true,
			"requestorName":"requestor-mac1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}],
		"users":[{
			"userName":"user1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
	}]
}

