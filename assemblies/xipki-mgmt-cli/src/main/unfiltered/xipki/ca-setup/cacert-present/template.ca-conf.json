{
	"basedir":"APP_DIR",
	"properties":{
		"profiles.dir":"xipki/ca-setup/certprofile",
		"token.type":"REPLACEME-TOKEN_TYPE",
		"ca.signerConf":"REPLACEME-CA_SIGNERCONF"
	},
	"signers":[{
		"name":"responder1",
		"type":"pkcs12",
		"conf":{
			"value":"password=1234,keystore=file:xipki/keycerts/tlskeys/server/tls-server.p12,algo=SHA256withECDSA"
		}
	}],
	"keypairGens":[],
	"requestors":[{
		"name":"requestor1",
		"type":"cert",
		"binaryConf":{
			"file":"xipki/keycerts/tlskeys/client/tls-client-cert.der"
		}
	}],
	"publishers":[
//	      {
//	        "name":"ocsp-publisher",
//	        "type":"ocsp",
//	        "conf":{
//	            "value":"datasource=ocsp,publish.goodcerts=true"
//	        }
//        }
	],
	"profiles":[{
		"name":"scep",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-scep.json"
		}
	},{
		"name":"ocsp",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-ocsp.json"
		}
	},{
		"name":"smime",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-smime.json"
		}
	},{
		"name":"tls",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-tls.json"
		}
	},{
		"name":"tls-c",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-tls-c.json"
		}
	}],
	"cas":[{
		"name":"myca1",
		"aliases":[
			"myca"
		],
		"caInfo":{
			"caUris":{
				"crlUris":[
					"http://ca.myorg.org/crl/myca1"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp/"
				]
			},
			"popControl":{
				"value":"sigalgo=SHA256withRSA:SHA256withECDSA:SHA256withDSA:SM3withSM2:ED25519:ED448:DHPOP-X25519:DHPOP-X448"
			},
			"cmpResponderName":"responder1",
			"keypairGenNames": ["software"],
			"crlControl":{
				"interval.hours": 24,
				"fullcrl.intervals":7,
				"overlap":"3d",
				"interval.time":"02:00"
			},
			"expirationPeriod":365,
			"cert":{
				"file":"xipki/ca-setup/keycerts/myca1.der"
			},
			"keepExpiredCertDays":-1,
			"maxValidity":"10y",
			"nextCrlNo":2,
			"permissions":["all"],
			"protocolSupport":["cmp","rest","scep"],
			"saveCert": true,
			"saveRequest":false,
			"saveKeypair":false,
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":20,
			"status":"active",
			"validityMode":"cutoff"
		},
		"profiles":[
			"ocsp",
			"scep",
			"smime",
			"tls",
			"tls-c"
		],
		"publishers":[
//	        "ocsp-publisher"
		],
		"requestors":[{
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
	}]
}

