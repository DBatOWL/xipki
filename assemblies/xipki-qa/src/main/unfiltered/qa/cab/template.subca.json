{
    "basedir":"APP_DIR",
	"properties":{
		"token.type":"REPLACEME-TOKEN_TYPE",
		"ca.signerConf": "REPLACEME-CA_SIGNERCONF"
	},
	"cas":[{
		"name":"subca1",
		"aliases":[
			"subca"
		],
		"caInfo":{
			"caUris":{
				"cacertUris":[
					"http://myorg.org/subca1.der"
				],
				"crlUris":[
					"http://localhost:8080/crl/?type=crl&name=subca1"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp/responder1"
				]
			},
			"certchain":[{
				"file":"output/rootca1.crt"
			}],
			"cmpControl":{
				"send.ca":true,
				"send.certchain":false,
				"messagetime.bias":300,
				"rr.aki.required":true,
				"group.enroll":true,
				"protection.sigalgo":"REPLACEME-CMP_SIGALGO",
				"popo.sigalgo":"REPLACEME-SIGALGO:REPLACEME-HASHwithRSA:REPLACEME-HASHwithECDSA:REPLACEME-HASHwithDSA:SM3withSM2"
			},
			"cmpResponderName":"responder1",
			"keypairGenNames":["software"],
			"ctlogControl":{
				"enabled":true,
				"servers":"https://localhost:8443/ctlog/ctlogrsa;https://localhost:8443/ctlog/ctlogec",
				"sslcontext.name":"localhost"
			},
			"expirationPeriod":365,
			"revokeSuspendedControl":{
				"enabled":true,
				"targetReason":"cessationOfOperation",
				"unchangedSince":"15d"
			},
			"keepExpiredCertDays":-1,
			"maxValidity":"5y",
			"nextCrlNo":2,
			"permissions":["all"],
			"protocolSupport":["cmp","rest"],
			"saveRequest":false,
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":20,
			"status":"active",
			"validityMode":"strict"
		},
		"profiles":[
			"cab-domain-validated",
			"cab-org-validated",
			"cab-individual-validated"
		],
		"publishers":[
			"ocsp-publisher"
		],
		"requestors":[{
			"ra":true,
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		},{
			"ra":true,
			"requestorName":"requestor-mac1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
	}]
}

