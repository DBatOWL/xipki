		"pbmMac":{
			"macAlgos":["HMAC-SHA256"],
			"owfAlgos":["SHA256"]
		},
		"signature":{
			"signatureAlgos":["SHA256WITHECDSA"],
			"cert":{
				"file":"xipki/keycerts/gateway-server-cert.pem"
			}
		},
		"dhPopCerts":{
			"file":"qa/keys/dhpop-certs.pem"
		}
	},
	"ssl":{
		"name":"ssl1",
		"hostnameVerifier":"default",
